
<%
 #puts "Element: #{element.resource_type}"
 heading = element.resource
 #puts "Heading Size: #{heading.size}"
 case heading.size
	when 1
%>
	<h1><%= heading.value%></h1>
	<% when 2%>
	<h2><%= heading.value%></h2>
	<% when 3%>
	<h3><%= heading.value%></h3>
	<% when 4%>
	<h4><%= heading.value%></h4>
	<% when 5%>
	<h5><%= heading.value%></h5>
	<% when 6%>
	<h6><%= heading.value%></h6>
	<% else %>
	<p>This isn't a valid HTML heading</p>
	<% end %>